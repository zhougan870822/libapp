<?xml version="1.0" encoding="utf-8"?>
<resources>
    <paths>
        <root-path
            name="root"
            path="" />

        <comFile-path
            name="comFile"
            path="." /><!--Context.getFilesDir():/data/data/package/comFile 非root权限看不见此目录-->
        <cache-path
            name="cache"
            path="." /> <!--getCacheDir():/data/data/package/cache -->
        <external-comFile-path
            name="externalFiles"
            path="." /><!--表示应用程序外部存储区域根目录中的文件。
        该子目录的根路径与Context＃getExternalFilesDir（String）Context.getExternalFilesDir（null）返回的值相同。
        即:SDCard/Android/data/package/comFile-->
        <external-cache-path
            name="externalCache"
            path="." /><!--Context.getExternalCacheDir():SDCard/Android/data/package/cache-->
        <external-path
            name="sdcard"
            path="" /><!--Environment.getExternalStorageDirectory():SDCard/-->
        <!--一般情况SDCard: /storage/emulated/0-->
    </paths>
</resources>

